{
  "name": "phishing-detection-system",
  "version": "1.0.0",
  "description": "A comprehensive phishing detection and response system with React frontend and Node.js backend",
  "main": "backend/server.js",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start": "npm run backend:start",
    "build": "npm run frontend:build",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "frontend:dev": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "install-all": "npm install && npm run backend:install && npm run frontend:install",
    "backend:install": "cd backend && npm install",
    "frontend:install": "cd frontend && npm install",
    "test": "npm run backend:test && npm run frontend:test",
    "backend:test": "cd backend && npm test",
    "frontend:test": "cd frontend && npm test",
    "clean": "npm run backend:clean && npm run frontend:clean",
    "backend:clean": "cd backend && rm -rf node_modules",
    "frontend:clean": "cd frontend && rm -rf node_modules build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "phishing",
    "detection",
    "security",
    "cybersecurity",
    "react",
    "nodejs",
    "typescript",
    "virustotal"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/phishing-detection-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/phishing-detection-system/issues"
  },
  "homepage": "https://github.com/yourusername/phishing-detection-system#readme"
}
