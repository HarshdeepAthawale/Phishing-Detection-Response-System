{
  "name": "phishing-detection-system",
  "version": "1.0.0",
  "description": "A phishing detection and response system",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "url-parse": "^1.5.10",
    "whois": "^2.13.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["phishing", "detection", "security", "cybersecurity"],
  "author": "Your Name",
  "license": "MIT"
}
